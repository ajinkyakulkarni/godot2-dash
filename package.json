{
  "name": "godot2-dash",
  "version": "0.2.131",
  "description": "HTTP dashboard for Godot2; A port of riemann/riemann-dash",
  "author": "Joshua Holbrook",
  "main": "./index.js",
  "scripts": {
    "start": "node ./bin/godot-dash",
    "css": "lessc ./less/css.less ./public/css.css",
    "engine.io-client": "curl https://raw.githubusercontent.com/socketio/engine.io-client/1.6.11/engine.io.js -o public/vendor/engine.io.js",
    "watch": "iced --watch --bare --map --runtime node -o ./lib -c src/",
    "build": "iced --bare --map --runtime node -o ./lib -c src/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/aphyr/riemann-dash.git"
  },
  "keywords": [
    "godot",
    "dash",
    "dashboard"
  ],
  "license": "MIT",
  "dependencies": {
    "engine.io": "^1.6.11",
    "riemann-query-parser": "^1.0.1"
  },
  "devDependencies": {
    "iced-coffee-script": "^108.0.11",
    "less": "^2.7.1"
  },
  "peerDependencies": {
    "ecstatic": "^2.1.0",
    "godot2": "^1.9.9"
  }
}
